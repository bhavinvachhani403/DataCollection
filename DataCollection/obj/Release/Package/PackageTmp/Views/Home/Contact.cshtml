@{
    ViewBag.Title = "DOAA DATA PROFORMA FOR RANKING";
}

<div class="direction">
    <h3>DOAA DATA PROFORMA FOR RANKING</h3>  
</div>
<div class="bd-example">
    <div class="table-responsive-xl">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Ph.D Student Details</th>
                    <th scope="col" width="10%">Full Time (FT)</th>
                    <th scope="col" width="10%">Part Time (PT)</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>Total Students in Financial Year</td>
                    <td><input type="text" class="form-control" /></td>
                    <td><input type="text" class="form-control" /></td>
                </tr>
                <tr>
                    <td>No. of Ph.D students graduated in Financial Year (including Integrated Ph.D)</td>
                    <td><input type="text" class="form-control" /></td>
                    <td><input type="text" class="form-control" /></td>
                </tr>


                <tr>
                    <td scope="col">Tuition Fees</td>
                    <td scope="col" width="10%">Domestic</td>
                    <td scope="col" width="10%">International</td>
                </tr>


                <tr>
                    <td>Undergraduate Fees</td>
                    <td><input type="text" class="form-control" /></td>
                    <td><input type="text" class="form-control" /></td>
                </tr>
                <tr>
                    <td>Postgraduate Fees</td>
                    <td><input type="text" class="form-control" /></td>
                    <td><input type="text" class="form-control" /></td>
                </tr>
                <tr>
                    <td>Ph. D</td>
                    <td><input type="text" class="form-control" /></td>
                    <td><input type="text" class="form-control" /></td>
                </tr>

                <tr>
                    <td>Regional Rankings Indicator</td>
                    <td>Full Time</td>
                    <td>Part Time</td>
                </tr>

                <tr>
                    <td>Undergraduate Exchange Students - Inbound</td>
                    <td><input type="text" class="form-control" /></td>
                    <td><input type="text" class="form-control" /></td>
                </tr>
                <tr>
                    <td>Undergraduate Exchange Students - Outbound</td>
                    <td><input type="text" class="form-control" /></td>
                    <td><input type="text" class="form-control" /></td>
                </tr>
                <tr>
                    <td>Postgraduate Exchange Students - Inbound</td>
                    <td><input type="text" class="form-control" /></td>
                    <td><input type="text" class="form-control" /></td>
                </tr>
                <tr>
                    <td>Postgraduate Exchange Students - Outbound</td>
                    <td><input type="text" class="form-control" /></td>
                    <td><input type="text" class="form-control" /></td>
                </tr>
                <tr>
                    <td>Student Exchange Outbound - Students who have attended another university abroad on international exchange programmes for at least one semester during the annual reporting period</td>
                    <td><input type="text" class="form-control" /></td>
                    <td><input type="text" class="form-control" /></td>
                </tr>
                <tr>
                    <td>Student Exchange Inbound - Students who have attended the institute on international exchange programmes for at least one semester during the annual reporting period</td>
                    <td><input type="text" class="form-control" /></td>
                    <td><input type="text" class="form-control" /></td>
                </tr>
            </tbody>
        </table>
    </div>
</div>

<ul class="list-inline">
    <li class="list-inline-item" style="color: #933;font-weight:bold">Data Status Level: </li>
    <li class="list-inline-item"><input type="text" readonly="readonly" class="form-control" value="Data entry level" /></li>
    <li class="list-inline-item" style="float:right"><button type="button" class="btn btn-primary">Preview</button></li>
    <li class="list-inline-item" style="float:right"><button type="button" class="btn btn-primary">Finalize</button></li>
    <li class="list-inline-item" style="float:right"><button type="button" class="btn btn-primary">Save</button></li>
</ul>